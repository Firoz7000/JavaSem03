import java.util.*;

public class PrimeNo {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num = sc.nextInt();
        sc.close();

        boolean checkP = true;
        int i = 2;

        while (i < num) {
            if (num % i == 0) {
                checkP = false;
                break;
            }
            i++;
        }

        if (num <= 1)
            checkP = false;

        if (checkP) {
            System.out.println("Prime");
        } else {
            System.out.println("Not Prime");
        }
    }
}
